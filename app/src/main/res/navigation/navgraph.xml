            android:id="@+id/action_foodHomeFragment2_to_orderListFragment2"
            app:destination="@id/orderListFragment2" />
        <action
            android:id="@+id/action_foodHomeFragment2_to_feedbackFragments"
            app:destination="@id/feedbackFragments" />
        <action
            android:id="@+id/action_foodHomeFragment2_to_cartListFragment"
            app:destination="@id/cartListFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="uk.ac.tees.w9544151.Passenger.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
