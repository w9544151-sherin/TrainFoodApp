            android:id="@+id/action_loginFragment_to_foodHomeFragment2"
            app:destination="@id/foodHomeFragment2" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
